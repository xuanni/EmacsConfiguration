! swap left ctrl and left alt key for Emacs
! this file will be loaded by xmodmap automatically



clear control
clear mod1
keycode 37 = Alt_L Meta_L
keycode 64 = Control_L
add control = Control_L Control_R
add mod1 = Alt_L Meta_L Alt_R Meta_R


